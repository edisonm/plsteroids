echo ""
echo ""
make $1 clean OS_TYPE=LINUX
cp ../LIBRARY/gcc000libbid.a ../LIBRARY/libbid.a
ranlib ../LIBRARY/libbid.a
make $1 OS_TYPE=LINUX CC=gcc CALL_BY_REF=0 GLOBAL_RND=0 GLOBAL_FLAGS=0 UNCHANGED_BINARY_FLAGS=0
./readtest < readtest.in
