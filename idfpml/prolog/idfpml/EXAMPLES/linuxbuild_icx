echo ""
echo ""
echo "***************** RUNNING EXAMPLE FOR icx 000 **************************"
echo ""
echo ""
cp main.c_000 main.c
cp decimal.h_000 decimal.h
cp ../LIBRARY/icx000libbid.a ../LIBRARY/libbid.a
icx $1 main.c ../LIBRARY/libbid.a
./a.out
rm a.out
