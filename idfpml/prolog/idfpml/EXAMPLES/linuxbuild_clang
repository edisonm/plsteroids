echo ""
echo ""
echo "***************** RUNNING EXAMPLE FOR clang 000 **************************"
echo ""
echo ""
cp main.c_000 main.c
cp decimal.h_000 decimal.h
cp ../LIBRARY/clang000libbid.a ../LIBRARY/libbid.a
clang $1 main.c ../LIBRARY/libbid.a
./a.out
rm a.out
